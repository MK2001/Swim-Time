<h1>Editing Swimmer #<%= @swimmer.id %></h1>

<form action="/swimmers/<%= @swimmer.id %>/update">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" value="<%= @swimmer.name  %>" type="text" />
  </div>
  <div>
    <label for="username">username</label><br />
    <input id="username" name="username" value="<%= @swimmer.username  %>" type="text" />
  </div>
  <div>
    <label for="password">password</label><br />
    <input id="password" name="password" value="<%= @swimmer.password  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>


